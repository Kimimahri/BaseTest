<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Clipboard">
    <test name="Clipboard">
        <parameter name="clipboardPageUrl" value="https://clipboardjs.com"/>
        <parameter name="googlePageUrl" value="https://www.google.ru/"/>
        <parameter name="timeout" value="10"/>
        <parameter name="driverPath" value="src/resources/chromedriver.exe"/>
        <parameter name="text" value="Default Message"/>
        <classes>
            <class name="ClipboardTest"/>
        </classes>
    </test>
</suite>
